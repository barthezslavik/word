<p id="notice"><%= notice %></p>

<table style="">
  <tbody>
    <% @entities.each do |entity| %>
      <tr>
        <td><%= entity.id %></td>
        <td><%= entity.english %></td>
        <td><%= check_box_tag entity.done_article %></td>
        <td><%= entity.article %></td>
        <td><%= check_box_tag entity.done_german %></td>
        <td><%= entity.german %></td>
        <td><%= entity.done_article %></td>
        <td><%= entity.done_german %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entity', new_entity_path %>
